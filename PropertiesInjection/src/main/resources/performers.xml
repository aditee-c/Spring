<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd ">
	<!-- configure only classes not interfaces -->
	<!-- can map with jsp usebean -->
	<bean id="Saif Ali Khan" class="com.zensar.spring.performers.Instrumentalist">
	<!-- Setter Injection -->
		<property name="instrumentSongProps">
			<props>
					<prop key="guitar">Hum toh udd chle</prop>
					<prop key="piano">Love Story</prop>
					<prop key="tabla">Cheap Thrills</prop>
			</props>
		</property>
		

	</bean>
	
</beans>
